{
  "manifest_version": 2,
  "name": "Focus",
  "version": "0.0.2",
  "description": "Focus on your work",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
],
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "icons": {
    "192": "logo192.png"
  },
  "browser_action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
